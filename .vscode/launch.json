  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch File Crawler",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/file-crawler/index.ts",
      "preLaunchTask": "tsc: build - file-crawler/tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/file-crawler/**/*.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
